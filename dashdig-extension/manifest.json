{
  "manifest_version": 3,
  "name": "Dashdig - Smart URL Shortener",
  "version": "1.0.0",
  "description": "Create memorable, human-readable short links with one click",
  "permissions": [
    "activeTab",
    "storage",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://dashdig-production.up.railway.app/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}









